algoritmo "deltador"
var
    a: real
    b: real
    c: real
    delta: real

inicio
    escreva("Digite o valor de A: ")
    leia(a)
    escreva("Digite o valor de B: ")
    leia(b)
    escreva("Digite o valor de C: ")
    leia(c)

    delta <- b^2 - 4 * a * c 

    escreval("Seu delta Ã©: ", delta)

fimalgoritmo